{
    "name": "vuanem-insider-export",
    "version": "1.0.0",
    "dependencies": {
        "@google-cloud/dataflow": "^1.2.0",
        "@google-cloud/functions-framework": "^3.0.0",
        "axios": "^0.26.0",
        "dayjs": "^1.10.8",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "dotenv": "^16.0.0",
        "eslint": "^8.9.0",
        "jest": "^27.5.1",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.5"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testRegex": "./test/.*.ts$",
        "setupFiles": [
            "dotenv/config"
        ]
    },
    "eslintConfig": {
        "env": {
            "es2021": true,
            "node": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "ecmaVersion": "latest",
            "sourceType": "module"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "rules": {
            "@typescript-eslint/no-unused-vars": 0,
            "@typescript-eslint/no-explicit-any": 0
        }
    }
}
